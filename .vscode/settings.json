{
    "git.ignoreLimitWarning": true,
    // "eslint.validate": [
    //   "javascript",
    //   "javascriptreact",
    //   "typescript"
    // ],
    "typescript.tsdk": "node_modules/typescript/lib",
    "editor.codeActionsOnSave": {
      "source.fixAll": true
    },
    "emmet.includeLanguages": {
      "javascript": "javascriptreact"
    },
    "[javascript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
}